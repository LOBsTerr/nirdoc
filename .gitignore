# gitignore template for Drupal 8 projects
#
# earlier versions of Drupal are tracked in `community/PHP/

# Ignore files generated by IDEs.
/.idea/
/.vscode/

# Ignore files generated by installation script.
/config/sync/.htaccess

# Ignore local configuration files.
/behat.yml
/grumphp.yml
/runner.yml
/docker-compose.*.yml
/config.extra.yml
.env

# Ignore data folder.
/data/

# Ignore test artifacts
/tests/screenshots/*.html
/tests/screenshots/*.png

# Ignore database artifacts.
*.sql.gz
*.sql
/.gitattributes

# Ignore drupal core (if not versioning drupal sources)
/web/core
/web/libraries
/web/modules/contrib
/web/profiles
/web/sites
/web/themes/contrib
web/.csslintrc
web/.eslintignore
web/.eslintrc.json
web/.gitignore
web/.ht.router.php
web/.htaccess
web/INSTALL.txt
web/README.txt
web/example.gitignore
web/index.php
web/robots.txt
web/themes/README.txt
web/update.php
web/web.config
web/modules/README.txt
/vendor/
